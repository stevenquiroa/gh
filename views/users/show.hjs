{{<layout}}
	{{$title}}<h1>{{title}}</h1>{{/title}}
	{{$content}}
		<h2>{{username}}</h2>
		<p><b>Nombre:</b> {{user.name}}</p>
		<p><b>Username:</b> {{user.username}}</p>
		<p><b>Nacimiento:</b> {{user.birth}}</p>
		<p><b>Red:</b> {{user.social}}</p>
		<a href="/users">Regresar</a>
	{{/content}}
{{/layout}}